{
  "name": "planntt",
  "version": "0.0.98",
  "author": "Stijn Heymans <stijn.heymans@gmail.com>",
  "description": "A Simple ToDo Application",
  "homepage": "http://planntt.com",
  "license": "UNLICENSED",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "buildwin": "./src/scripts/win_cert.sh",
    "buildlin": "node .electron-vue/build.js && electron-builder -l",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "planntt",
    "appId": "com.planntt",
    "copyright": "Copyright Â© 2018-2021 Stijn Heymans",
    "directories": {
      "output": "build"
    },
    "afterSign": "scripts/notarize.js",
    "publish": [
      {
        "provider": "generic",
        "url": "http://planntt.com/downloads"
      }
    ],
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icons/planntt_icon.icns",
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "dependencies": {
    "@chartshq/muze": "^2.0.0",
    "@electron/remote": "^1.2.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "axios": "^0.21.1",
    "csv-parser": "^2.3.2",
    "electron-log": "^2.2.14",
    "electron-updater": "^4.2.0",
    "marked": "^0.3.19",
    "nedb": "^1.8.0",
    "npm": "^6.14.8",
    "save": "^2.3.2",
    "typeface-jetbrains-mono": "^1.0.5",
    "typeface-raleway": "0.0.54",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "vue": "^2.6.10",
    "vue-context-menu": "^2.0.6",
    "vue-directive-tooltip": "^1.6.3",
    "vue-electron": "^1.0.6",
    "vue-js-modal": "^1.3.26",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.1.3",
    "vue2-datepicker": "^2.4.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "ajv": "^8.6.0",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.14.5",
    "babel-loader": "^8.2.2",
    "cfonts": "^2.1.2",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "del": "^6.0.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "electron-debug": "^1.5.0",
    "electron-notarize": "^0.2.1",
    "eslint": "^7.28.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-vue": "^7.16.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "1.6.0",
    "html-webpack-plugin": "^5.3.1",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "5.1.3",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.7",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vue-eslint-parser": "^7.10.0",
    "webpack-cli": "^4.7.2",
    "webpack": "^5.49.0",
    "webpack-dev-server": "^4.0.0-rc.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0"
  }
}
